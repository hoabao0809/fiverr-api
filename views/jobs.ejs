<%- include('./included/header.ejs') %>
    <title>Document</title>
  </head>
  <body>
    <div class="container">
      <h1 class="centered">Post jobs</h1>
      <form action="jobs" method="POST">
        <div class="form-group">
          <label for="name">Name</label>
          <input
            type="text"
            class="form-control form-control-sm"
            name="name"
            id="name"
            placeholder="name"
          />
        </div>

        <div class="form-group">
          <label for="rating">Rating</label>
          <input
            type="text"
            class="form-control form-control-sm"
            name="rating"
            id="rating"
            placeholder="rating"
          />
        </div>
        <div class="form-group">
          <label for="price">Price</label>
          <input
            type="text"
            class="form-control form-control-sm"
            name="price"
            id="price"
            placeholder="price"
          />
        </div>
        <div class="form-group">
          <label for="proServices">ProServices</label>
          <input
            type="text"
            class="form-control form-control-sm"
            name="proServices"
            id="proServices"
            placeholder="proServices"
          />
        </div>
        <div class="form-group">
            <label for="localSellers">LocalSellers</label>
            <input
              type="text"
              class="form-control form-control-sm"
              name="localSellers"
              id="localSellers"
              placeholder="localSellers"
            />
          </div>
          <div class="form-group">
            <label for="onlineSellers">OnlineSellers</label>
            <input
              type="text"
              class="form-control form-control-sm"
              name="onlineSellers"
              id="onlineSellers"
              placeholder="onlineSellers"
            />
          </div>
          <div class="form-group">
            <label for="deliveryTime">DeliveryTime</label>
            <input
              type="text"
              class="form-control form-control-sm"
              name="deliveryTime"
              id="deliveryTime"
              placeholder="deliveryTime"
            />
          </div>
          <div class="form-group">
            <label for="type">Type</label>
            <input
              type="text"
              class="form-control form-control-sm"
              name="type"
              id="type"
              placeholder="type"
            />
          </div>
          <div class="form-group">
            <label for="subType">SubType</label>
            <input
              type="text"
              class="form-control form-control-sm"
              name="subType"
              id="subType"
              placeholder="subType"
            />
          </div>
          <div class="form-group">
            <label for="userCreated">UserCreated</label>
            <input
              type="text"
              class="form-control form-control-sm"
              name="userCreated"
              id="userCreated"
              placeholder="userCreated"
            />
          </div>
        <button type="submit">Post</button>
      </form>
    </div>
<%- include('./included/footer.ejs') %>
